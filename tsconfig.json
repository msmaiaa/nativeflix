{
  "compilerOptions": {
    "jsx": "react-native",
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
	"noImplicitAny": true,
	"noUnusedLocals": true,
	"noUnusedParameters": true,
  },
  	"include":[
		"src", "decs.d.ts", "./App.tsx",
    "**/*",
	]
}
